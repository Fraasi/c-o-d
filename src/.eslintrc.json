{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": 0,
        "semi": 0,
        "indent": 0,
        "no-tabs": 0,
        "no-use-before-define": 0,
        "no-underscore-dangle": 0,
        "no-plusplus": 0,
        "no-nested-ternary": 0,
        "func-names": 0,
        "object-curly-newline": 0,
        "max-len": 0,
        "prefer-template": 0
    }
}